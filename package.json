{
  "name": "sierraalfawhiskey",
  "version": "0.0.1",
  "description": "The SierraAlfaWhiskey website (It's me! It's mine!)",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "dev:h": "export NODE_ENV=development && astro dev --host",
    "start": "npm run dev",
    "start:h": "npm run dev:h",
    "build": "export NODE_ENV=production && astro build",
    "build:p": "npm run build && npm run preview",
    "build:ph": "npm run build && npm run preview:h",
    "preview": "astro preview",
    "preview:h": "astro preview --host"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottaw66/sierraalfawhiskey.git"
  },
  "keywords": [
    "astro",
    "blog",
    "wiki",
    "links",
    "braindump"
  ],
  "author": "Scott Willsey",
  "license": "GPL",
  "dependencies": {
    "@astrojs/rss": "^2.3.1",
    "@astrojs/sitemap": "^1.2.1",
    "astro": "^2.1.7",
    "astro-embed": "^0.2.1",
    "astro-icon": "^0.8.0",
    "astro-pagefind": "file:../astro-components/astro-pagefind",
    "config": "^3.3.9",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "nanoid": "^4.0.1",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "astro-eslint-parser": "^0.13.2",
    "eslint": "^8.36.0",
    "eslint-plugin-astro": "^0.26.1",
    "prettier": "^2.8.7",
    "prettier-plugin-astro": "^0.8.0"
  }
