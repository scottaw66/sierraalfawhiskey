{
  "name": "sierraalfawhiskey",
  "version": "0.0.1",
  "description": "The SierraAlfaWhiskey website (It's me! It's mine!)",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "dev:h": "export NODE_ENV=development && astro dev --host",
    "start": "npm run dev",
    "start:h": "npm run dev:h",
    "build": "export NODE_ENV=production && astro build && npx -y pagefind@latest",
    "build:p": "npm run build && npm run preview",
    "build:ph": "npm run build && npm run preview:h",
    "preview": "astro preview",
    "preview:h": "astro preview --host"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottaw66/sierraalfawhiskey.git"
  },
  "keywords": [
    "astro",
    "blog",
    "wiki",
    "links",
    "braindump"
  ],
  "author": "Scott Willsey",
  "license": "GPL",
  "dependencies": {
    "@astrojs/image": "^1.0.0-beta.2",
    "@astrojs/rss": "^2.0.0",
    "@astrojs/sitemap": "^1.0.0",
    "astro": "^2.0.0-beta.2",
    "astro-icon": "^0.8.0",
    "config": "^3.3.8",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "nanoid": "^4.0.0",
    "normalize.css": "^8.0.1",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "astro-eslint-parser": "^0.9.5",
    "eslint": "^8.32.0",
    "eslint-plugin-astro": "^0.21.1",
    "prettier": "^2.8.3",
    "prettier-plugin-astro": "^0.7.2"
  }
}
