{
  "name": "sierraalfawhiskey",
  "version": "0.0.1",
  "description": "The SierraAlfaWhiskey website (It's me! It's mine!)",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "dev:h": "export NODE_ENV=development && astro dev --host",
    "start": "npm run dev",
    "start:h": "npm run dev:h",
    "build": "export NODE_ENV=production && astro build",
    "build:p": "npm run build && npm run preview",
    "build:ph": "npm run build && npm run preview:h",
    "preview": "astro preview",
    "preview:h": "astro preview --host"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottaw66/sierraalfawhiskey.git"
  },
  "keywords": [
    "astro",
    "blog",
    "wiki",
    "links",
    "braindump"
  ],
  "author": "Scott Willsey",
  "license": "GPL",
  "dependencies": {
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "astro": "^3.4.4",
    "astro-embed": "^0.6.0",
    "astro-icon": "^0.8.1",
    "astro-pagefind": "^1.3.0",
    "config": "^3.3.9",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "nanoid": "^5.0.3",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "astro-eslint-parser": "^0.16.0",
    "eslint": "^8.53.0",
    "eslint-plugin-astro": "^0.29.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.1"
  }
}
